{
  "name": "GR+VI",
  "version": "2022.12.25-07.32.47",
  "license": "MIT",
  "homepage": "https://codepen.io/JJewell2k/pen/vYaNgZb",
  "scripts": {
    "build": "gulp ./build/gulpfile.js --cwd ./ --continue",
    "start": "gulp start ./build/gulpfile.js --cwd ./ --continue",
    "task": "gulp -f ./build/gulpfile.js --cwd ./ --continue",
    "tasks": "gulp --tasks-simple ./build/gulpfile.js --cwd ./ --continue"
  },
  "config": {
    "srcDir": "src/",
    "distDir": "dist/"
  },
  "devDependencies": {
    "browser-sync": "^2.27.11",
    "del": "^4.1.1",
    "glob": "^7.2.3",
    "gulp": "^4.0.2",
    "gulp-inject-partials": "^1.0.5",
    "gulp-rename": "^2.0.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "browserslist": [
    "last 5 version"
  ],
  "tasks": {
    "partials": {
      "order": 999,
      "src": [
        "**/*.template.html"
      ],
      "pipe": [
        {
          "require": "gulp-inject-partials",
          "args": [
            {
              "removeTags": true,
              "quiet": true,
              "ignoreError": true
            }
          ]
        }
      ],
      "rename": {
        "find": ".template",
        "replace": ""
      },
      "cleanup": [
        "**/*.partial.html",
        "**/*.template.html"
      ]
    }
  }
}
